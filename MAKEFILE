release: DRAWING.ASM VIDEO.OBJ RANDOM.OBJ KEYB.OBJ
	ML /W3 /WX VIDEO.OBJ RANDOM.OBJ KEYB.OBJ DRAWING.ASM /Fe DRAWING.EXE

debug: DRAWING.ASM VIDEOD.OBJ RANDOM_D.OBJ KEYBD.OBJ
	ML /Zi /W3 /WX VIDEOD.OBJ RANDOM_D.OBJ KEYBD.OBJ DRAWING.ASM /Fe DRAWINGD.EXE

VIDEO.OBJ: VIDEO.ASM
	ML /c /W3 /WX /FoVIDEO.OBJ /Ta VIDEO.ASM
	
VIDEOD.OBJ: VIDEO.ASM
	ML /c /Zi /W3 /WX /FoVIDEOD.OBJ /Ta VIDEO.ASM
	
RANDOM.OBJ: RANDOM.ASM
	ML /c /W3 /WX /FoRANDOM.OBJ /Ta RANDOM.ASM
RANDOM_D.OBJ: RANDOM.ASM
	ML /c /Zi /W3 /WX /FoRANDOM_D.OBJ /Ta RANDOM.ASM
	
KEYB.OBJ: KEYB.ASM
	ML /c /W3 /WX /FoKEYB.OBJ /Ta KEYB.ASM
	
KEYBD.OBJ: KEYB.ASM
	ML /c /Zi /W3 /WX /FoKEYBD.OBJ /Ta KEYB.ASM

clean:
	del *.EXE
	del *.OBJ

cv: debug
	cv /s /43 DRAWINGD.EXE
